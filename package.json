{
  "name": "cli-test-harness",
  "version": "0.0.1",
  "description": "A test harness to invoke a CLI in a tmp directory",
  "keywords": [
    "cli",
    "fake",
    "project",
    "test",
    "harness"
  ],
  "license": "MIT",
  "author": "Steve Calvert <steve.calvert@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "docs:generate": "npm run build && readme-api-generator dist/index.js",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "watch": "npm run build -- --watch src"
  },
  "dependencies": {
    "execa": "^5.1.1",
    "fixturify-project": "^4.1.0"
  },
  "devDependencies": {
    "@scalvert/readme-api-generator": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "eslint-plugin-unicorn": "^41.0.0",
    "prettier": "^2.5.1",
    "tsup": "^5.12.0",
    "type-fest": "^2.12.0",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vitest": "^0.6.0"
  },
  "engines": {
    "node": ">=14"
  },
  "volta": {
    "node": "14.19.0",
    "npm": "8.5.3"
  }
}
